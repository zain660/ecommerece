    "require": {
        "php": "^8.0|^8.1",
        "gliterd/backblaze-b2": ">=1.5.0",
        "marcandreappel/flysystem-backblaze": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MarcAndreAppel\\BackblazeB2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcAndreAppel\\BackblazeB2\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\MarcAndreAppel\\BackblazeB2\\BackblazeB2ServiceProvider"
            ]
